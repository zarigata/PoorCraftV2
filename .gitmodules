# .gitmodules - Git submodules configuration for PoorCraft dependencies

[submodule "libs/glfw"]
    path = libs/glfw
    url = https://github.com/glfw/glfw.git
    branch = master

[submodule "libs/glm"]
    path = libs/glm
    url = https://github.com/g-truc/glm.git
    branch = master

[submodule "libs/enet"]
    path = libs/enet
    url = https://github.com/lsalzman/enet.git
    branch = master

[submodule "libs/imgui"]
    path = libs/imgui
    url = https://github.com/ocornut/imgui.git
    branch = master

[submodule "libs/lua"]
    path = libs/lua
    url = https://github.com/lua/lua.git
    branch = v5.4

[submodule "libs/sol2"]
    path = libs/sol2
    url = https://github.com/ThePhD/sol2.git
    branch = develop
